<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<key>@PLUGIN_KEY@</key>
	<version>@PLUGIN_VERSION@</version>
	<label>@PLUGIN_KEY@</label>
	<category>Resource Management</category>
	<description>Integration of Docker with ElectricCommander</description>
	<help>@PLUGIN_KEY@_help.xml</help>
	<author>Electric Cloud</author>
	<authorUrl>http://www.electric-cloud.com/support</authorUrl>
	<commander-version min="3.6"/>
	<customTypes>
		<customType name="DockerPull">
			<parameterPanel>
				<javascript>war/ecplugins.EC_Docker.DockerPullParameterPanel/ecplugins.EC_Docker.DockerPullParameterPanel.nocache.js</javascript>
			</parameterPanel>
		</customType>
	</customTypes>
	<customTypes>
		<customType name="DockerBuild">
			<parameterPanel>
				<javascript>war/ecplugins.EC_Docker.DockerBuildParameterPanel/ecplugins.EC_Docker.DockerBuildParameterPanel.nocache.js</javascript>
			</parameterPanel>
		</customType>
	</customTypes>
	<customTypes>
		<customType name="DockerRun">
			<parameterPanel>
				<javascript>war/ecplugins.EC_Docker.DockerRunParameterPanel/ecplugins.EC_Docker.DockerRunParameterPanel.nocache.js</javascript>
			</parameterPanel>
		</customType>
	</customTypes>
</plugin>
